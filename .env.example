# Development Environment (Local Backend)
# Uncomment these for local development with backend running on localhost:8080
# VITE_API_URL=http://localhost:8080/api/v1
# VITE_STATIC_BASE_URL=http://localhost:8080
# NODE_ENV=development

# Production Environment (Server Deployment)
# Use these for server deployment
VITE_API_URL=https://plp-api.moeys.gov.kh/api/v1
VITE_STATIC_BASE_URL=https://plp-api.moeys.gov.kh
NODE_ENV=production

# HTTPS Preference (set to false for local development, true for production)
VITE_PREFER_HTTPS=true